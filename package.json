{
  "name": "games-blog-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "start": "node dist/index.js",
    "start:docker": "pnpm build && pnpm start",
    "watch": "watchy --no-init-spawn --debounce 1 -w src -- docker-compose restart backend",
    "db:migrate": "knex migrate:latest",
    "docker:db:migrate": "docker compose run backend pnpm db:migrate",
    "docker:db:migrate:undo": "docker compose run backend pnpm knex migrate:down",
    "docker:seed:dev": "docker compose run backend pnpm ts-node scripts/seed/dev.ts",
    "rebuild:be": "docker compose build backend && docker compose rm --force --stop backend && docker compose up -d backend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "volta": {
    "node": "18.17.0"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "argon2": "^0.31.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.1",
    "typescript": "^5.1.6",
    "zennv": "^0.1.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.71",
    "@tsconfig/node18": "^18.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.17.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prisma": "^5.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "watchy": "^0.10.1"
  },
  "lint-staged": {
    "src/**/*.+(ts|js)": [
      "eslint"
    ]
  }
}
